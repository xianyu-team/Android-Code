<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chenweizhao.xianyu">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activities.AccountActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.PerfectInformationActivity" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.SetPasswordActivity" />
        <activity android:name=".Activities.QuestionnaireDesignActivity" />
        <activity android:name=".Activities.QuestionnaireShowActivity" />
        <activity android:name=".Activities.QuestionAddActivity"></activity>
        <activity android:name=".Activities.InfoActivity"></activity>
        <activity android:name=".Activities.AssignmentActivity"></activity>
        <activity android:name=".Activities.FollowActivity"></activity>
        <activity android:name=".Activities.FollowDetailActivity"></activity>
        <activity android:name=".Activities.SendTaskActivity"></activity>
    </application>

</manifest>